<?xml version="1.0"?>
<robot name="bottle_inference">
  <link name="bottle_inference">
    <visual>
      <origin xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="bottle_inference.obj" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="custom_color">
        <color rgba="1 0.85 0.2 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="bottle_inference.obj" scale="1.0 1.0 1.0"/>
      </geometry>
    </collision>
    <inertial>
      <!-- Mass value from the *empty* spam can. Full spam can is 368g -->
      <mass value="0.2"/> 
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
  </link>
</robot>
